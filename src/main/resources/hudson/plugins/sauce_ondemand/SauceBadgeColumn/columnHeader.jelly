<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core">
    <j:set var="disableColumn" value="${it.isColumnDisabled()}"/>
    <j:choose>
        <j:when test="${disableColumn}">
        </j:when>
        <j:otherwise>
            <th>
                ${%SauceBadgeColumn.Header}
            </th>
        </j:otherwise>
    </j:choose>
</j:jelly>